generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int     @id @default(autoincrement())
  firstName  String
  lastName   String
  email      String  @unique
  phone      String
  address    String
  image      String? // profile image path
  govtId     String? // govt ID image path
  createdAt  DateTime @default(now())
}
